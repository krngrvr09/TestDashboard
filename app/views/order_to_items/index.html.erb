<p id="notice"><%= notice %></p>

<h1>Listing Order To Items</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Item</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_to_items.each do |order_to_item| %>
      <tr>
        <td><%= order_to_item.order_id %></td>
        <td><%= order_to_item.item_id %></td>
        <td><%= link_to 'Show', order_to_item %></td>
        <td><%= link_to 'Edit', edit_order_to_item_path(order_to_item) %></td>
        <td><%= link_to 'Destroy', order_to_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order to item', new_order_to_item_path %>
